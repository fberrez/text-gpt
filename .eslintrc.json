{
  "env": {
    "commonjs": true,
    "es2021": true,
    "node": true
  },
  "extends": "xo",
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest"
  },
  "rules": {
    "new-cap": ["error", { "capIsNewExceptions": ["ObjectId"] }],
    "camelcase": ["error", {"allow": ["max_tokens"]}]
  }
}
